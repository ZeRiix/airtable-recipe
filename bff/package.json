{
    "name": "bff",
    "type": "module",
    "scripts": {
        "duplo:dev": "tsx --watch interfaces/http/main.ts",
        "duplo:start": "tsx interfaces/http/main.ts",
        "duplo:generate": "duplojs-types-codegen --import @duplojs/node/globals --include interfaces/http/routes/index.ts --output ../front/src/libs/client/types/duplojsTypesCodegen.d.ts",
        "test:lint": "eslint",
        "test:types": "tsc",
        "postinstall": "npm run duplo:generate"
    },
    "dependencies": {
        "@duplojs/core": "1.0.3",
        "@duplojs/http-client": "1.2.2",
        "@duplojs/node": "1.0.1",
        "@duplojs/utils": "0.3.1",
        "cookie": "1.0.2",
        "dotenv": "16.4.5",
        "dotenv-expand": "12.0.1"
    },
    "devDependencies": {
        "@duplojs/types-codegen": "1.0.1",
        "@types/cookie": "0.6.0",
        "@types/node": "22.13.10"
    }
}
